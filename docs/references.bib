@ARTICLE{Poisot2023,
  title        = {Network embedding unveils the hidden interactions in the
                  mammalian virome},
  author       = {Poisot, Timothée and Ouellet, Marie-Andrée and Mollentze,
                  Nardus and Farrell, Maxwell J and Becker, Daniel J and
                  Brierley, Liam and Albery, Gregory F and Gibb, Rory J and
                  Seifert, Stephanie N and Carlson, Colin J},
